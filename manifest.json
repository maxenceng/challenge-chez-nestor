{
  "manifest_version": 2,

  "name": "Chez Nestor Trello Addon",
  "description": "Improves Trello and other internal tools for our use-cases",
  "version": "1.0.0",
  "content_scripts": [{
    "js": ["jquery.js", "trello.js"],
    "matches": ["https://trello.com/b/*"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Chez Nestor"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
